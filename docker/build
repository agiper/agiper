docker rm -f $(docker ps -a -q)
docker rmi $(docker images -q)
set -e
docker build -t quay.io/agiper/base agiper-base/
docker build -t quay.io/agiper/java agiper-java/
docker build -t quay.io/agiper/tool agiper-tool/
docker build -t quay.io/agiper/nginx agiper-nginx/
docker build -t quay.io/agiper/redis agiper-redis/
docker build -t quay.io/agiper/dotnet agiper-dotnet/
docker build -t quay.io/agiper/mariadb agiper-mariadb/
docker build -t quay.io/agiper/elastic agiper-elastic/
docker build -t quay.io/agiper/rabbitmq agiper-rabbitmq/
docker build -t quay.io/agiper/teamcity agiper-teamcity/
docker build -t quay.io/agiper/dotnetapp agiper-dotnetapp/